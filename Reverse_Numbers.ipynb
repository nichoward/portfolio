      "source": [
        "#reverse the number \n",
        "\n",
        "def reverse_number(n):\n",
        "    rev_num = 0 \n",
        "    while(n > 0): \n",
        "        a = n % 10\n",
        "        rev_num = rev_num * 10 + a \n",
        "        n = int(n / 10)\n",
        "    return rev_num\n",
        "\n",
        "reverse_number(112)\n"
      ],
