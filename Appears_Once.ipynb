        "num_list = [2,6,3,2,3,5,6,6,7,7]\n",
        "\n",
        "def single_number(num_list):\n",
        "    for i in range(len(num_list)):\n",
        "        count = num_list.count(num_list[i])\n",
        "        if count == 1:\n",
        "            return(num_list[i])\n",
        "            break\n",
        "    \n",
        "single_number(num_list)"
