        "#palindrome detector \n",
        "\n",
        "def palindrome(num):\n",
        "    num_str = str(num)\n",
        "    stringlength=len(num_str) # calculate length of the list\n",
        "    slicedString=num_str[stringlength::-1]\n",
        "    return slicedString == num_str\n",
        "\n",
        "print(palindrome(-676))\n",
        "print(palindrome(676))"
